#include "elevator_interface.h"

class Elevator: public ElevatorInterface {
    
    public:

        bool isDoor_status() {return this.door_status;}

        void setDoor_status(bool door_status) {this.door_status = door_status; }

        double getLoad_weight() {return this.load_weight;}

        void setLoad_weight(double load_weight) {this.load_weight = load_weight; }

        int getFloor() {return this.floor;}

        void setFloor(int floor) {this.floor = floor;}

        bool isLight_status() {return this.light_status;}

        void setLight_status(bool light_status) {this.light_status = light_status;}

        double getCurrent_temp() {return this.current_temp;}

        void setCurrent_temp(double current_temp) {this.current_temp = current_temp;}

        double getMax_load_weight() {return this.max_load_weight;}

        void setMax_load_weight(double max_load_weight) {this.max_load_weight = max_load_weight;}

        int getMax_floor() {return this.max_floor;}

        void setMax_floor(int max_floor) {this.max_floor = max_floor;}

        double getMax_temp() {return this.max_temp;}

        void setMax_temp(double max_temp) {this.max_temp = max_temp;}

}